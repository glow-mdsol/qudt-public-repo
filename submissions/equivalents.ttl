@prefix quantitykind: <http://qudt.org/vocab/quantitykind/> .
@prefix qudt: <http://qudt.org/schema/qudt/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .


# Adding mEq as a unit for AmountOfSubstance, non-SI but frequently used in medicine

quantitykind:Equivalent
  a qudt:QuantityKind ;
  skos:broader quantitykind:AmountOfSubstance ;
  qudt:applicableUnit unit:EQ .

# Note: conversion to SI (mol) should take into account valence for the analyte

unit:EQ
  a qudt:Unit ;
  dcterms:description "An equivalent is the amount of a substance that will react with a certain number of hydrogen ions." ;
  qudt:hasQuantityKind quantitykind:Equivalent ;
  qudt:dbpediaMatch "http://dbpedia.org/resource/Equivalent_%28chemistry%29"^^xsd:anyURI ;
  qudt:informativeReference "https://en.wikipedia.org/wiki/Equivalent_(chemistry)"^^xsd:anyURI ;
  qudt:hasDimensionVector qkdv:A1E0L0I0M0H0T0D0 ;
  qudt:symbol "Eq" ;
  qudt:ucumCode "eq"^^qudt:UCUMcs ;
  rdfs:label "Equivalent"@en .


quantitykind:EquivalentsPerUnitVolume a qudt:QuantityKind ;
  skos:broader quantitykind:AmountOfSubstancePerUnitVolume ;
  qudt:applicableUnit unit:EQ-PER-L .

unit:EQ-PER-L 
  a qudt:DerivedUnit ;
  a qudt:Unit ;
  qudt:symbol "Eq/L" ;
  qudt:hasQuantityKind quantitykind:EquivalentsPerUnitVolume ;
  rdfs:label "Equivalents Per Litre"@en .


