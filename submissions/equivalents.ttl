@prefix quantitykind: <http://qudt.org/vocab/quantitykind/> .
@prefix qudt: <http://qudt.org/schema/qudt/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .


# Adding mEq as a unit for AmountOfSubstance, non-SI but frequently used in medicine

quantitykind:AmountOfSubstance
  a qudt:QuantityKind ;
  qudt:applicableUnit unit:EQ ;
  qudt:applicableUnit unit:MilliEQ .

# Note: conversion to SI (mol) should take into account valence for the analyte

unit:EQ
  a qudt:Unit ;
  dcterms:description "An equivalent is the amount of a substance that will react with a certain number of hydrogen ions." ;
  qudt:hasQuantityKind quantitykind:AmountOfSubstance ;
  qudt:dbpediaMatch "http://dbpedia.org/resource/Equivalent_%28chemistry%29"^^xsd:anyURI ;
  qudt:informativeReference "https://en.wikipedia.org/wiki/Equivalent_(chemistry)"^^xsd:anyURI ;
  qudt:hasDimensionVector qkdv:A1E0L0I0M0H0T0D0 ;
  qudt:symbol "Eq" ;
  qudt:ucumCode "eq"^^qudt:UCUMcs ;
  rdfs:label "Equivalent"@en .

unit:MilliEQ 
  a qudt:DerivedUnit ;
  a qudt:Unit ;
  dcterms:description "An equivalent is the amount of a substance that will react with a certain number of hydrogen ions." ;
  qudt:hasQuantityKind quantitykind:AmountOfSubstance ;
  qudt:symbol "mEq" ;
  rdfs:label "Milliequivalent"@en .

quantitykind:AmountOfSubstancePerUnitVolume
  a qudt:QuantityKind ;
  qudt:applicableUnit unit:MilliEQ-PER-L ;
  qudt:applicableUnit unit:MilliEQ-PER-DeciL ;
  qudt:applicableUnit unit:MilliEQ-PER-MilliL .

unit:MilliEQ-PER-L 
  a qudt:DerivedUnit ;
  a qudt:Unit ;
  qudt:symbol "mEq/L" ;
  qudt:hasQuantityKind quantitykind:AmountOfSubstancePerUnitVolume ;
  rdfs:label "Milliequivalent Per Litre"@en .

unit:MilliEQ-PER-DeciL 
  a qudt:DerivedUnit ;
  a qudt:Unit ;
  qudt:hasQuantityKind quantitykind:AmountOfSubstancePerUnitVolume ;
  qudt:symbol "mEq/dL" ;
  rdfs:label "Milliequivalent Per Decilitre"@en .

unit:MilliEQ-PER-MilliL 
  a qudt:DerivedUnit ;
  a qudt:Unit ;
  qudt:hasQuantityKind quantitykind:AmountOfSubstancePerUnitVolume ;
  qudt:symbol "mEq/mL" ;
  rdfs:label "Milliequivalent Per Millilitre"@en .

